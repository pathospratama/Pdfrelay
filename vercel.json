{
  "version": 2,
  "builds": [
    {
      "src": "api/process.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/process-pdf",
      "dest": "/api/process.py"
    },
    {
      "src": "/health",
      "dest": "/api/process.py?health=1"
    }
  ]
}
